<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form">
	<Dialog id="BarcodeDialog" title="Barcode Scanner" type="Message">
		<content>
			<VBox>
				<Input placeholder="Enter Serial Number" id="InpSrialno" ></Input>
			</VBox>
		</content>
		<beginButton>
			<Button text="GO" press="OnPressGobtn"/>
		</beginButton>
		<endButton>
			<Button text="Cancel" press="OnPressCancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>